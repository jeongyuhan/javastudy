package ex02_2d_array;

public class Quiz02 {

	public static void main(String[] args) {
		
		// 구구단의 결과만 2차원 배열에 저장하고 출력하기
		// 2 4 6....18
		// 3 6 9....27
		// .........81
		
		for(int dan = 2; dan < 10; dan++) {
			for(int n = 1; n < 10; n++) {
				int[][] ggd = new int[dan][n];
				System.out.println(ggd);
			}
		}
		
		
		
		
		

	}

}
