package ex02_String;

public class Ex02_StringBuilder {

	public static void main(String[] args) {
		
		// java.lang.StringBuilder
		
		// 문자열 생성
		StringBuilder sb = new StringBuilder("hello");
		
		// 문자열 추가 (핵심 메소드)
		sb.append(" ").append("java"); // .append()의 식으로 계속 추가가 가능하다.
		
		System.out.println(sb);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
