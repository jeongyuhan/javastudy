package quiz04;

public class Gun {

	private String model;
	private int bullet; //총알수
	private final int FULL_BULLET = 6; // 최대 6발 가능
	
	// constructor
	public Gun(String model, int bullet) {
		this.model = model;
		this.bullet = bullet;
	}
	
	public Gun(int bullet) {
		this.bullet = bullet;
	}
	
	
	public void info() {
		System.out.print(model + "에 " + bullet + "발 남았다.");
	}
	
	
	
	
	
}
