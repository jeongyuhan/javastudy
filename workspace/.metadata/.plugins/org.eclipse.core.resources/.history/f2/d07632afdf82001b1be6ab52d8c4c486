package quiz02;

public class Circle {

	// field 
	private Coord center;
	private double radius;
	
	public Circle(Coord center, double radius) {
		this.center = center;
		this.radius = radius;
	}
	
	public void info() {
		String[] ct = center.info();
		System.out.println("중심좌표 " + ct + ", 반지름" + radius + ", 넓이" + (Math.PI * Math.pow(radius, 2)));
	}
	
	
}
