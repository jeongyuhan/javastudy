package quiz03;

public class Singer {

	// field
	private String name;
	private Song song; // 궁극적인 목표 private Song[] songs;

	// constructor
	public Singer(String name) {
		this.name = name;
	}
	
	// method
	public void setSong(Song song) {
		
	}
	
	public void info() {
		System.out.println(name + "(" + song + ")");
	} 
	
}
