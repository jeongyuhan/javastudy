package quiz03;

public class Singer {

	// field
	private String name;
	private Song song; // 궁극적인 목표 private Song[] songs;

	// constructor
	public Singer(String name) {
		this.name = name;
	}

	// method
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setSong(Song song) {
		this.song = song;
	}

	public Song getSong() {
		return song;
	}

	public void info() {
		System.out.println(name);
		song.info();
	}

}
